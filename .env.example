# Google Calendar Configuration
WORK_CALENDAR_ID=your-work-calendar-id@group.calendar.google.com
PERSONAL_CALENDAR_ID=your-personal-calendar-id@gmail.com

# Nextcloud Calendar Configuration
NEXTCLOUD_CALENDAR_URL=https://next.cloud/remote.php/dav/calendars/psamim/mine/
NEXTCLOUD_USERNAME=your-nextcloud-username
NEXTCLOUD_PASSWORD=your-nextcloud-password

# Sync Configuration
DAYS_TO_SYNC_PAST=7
DAYS_TO_SYNC_FUTURE=30
SYNC_INTERVAL_MINUTES=30
EVENT_PREFIX=Work:

# Token Paths (optional - defaults provided)
WORK_TOKEN_PATH=tokens/work_token.json
PERSONAL_TOKEN_PATH=tokens/personal_token.json

# Data File Paths (optional - defaults provided)
SYNCED_PERSONAL_EVENTS_FILE=data/synced_personal_events.json
SYNCED_NEXTCLOUD_EVENTS_FILE=data/synced_nextcloud_events.json 